<?xml version="1.0" encoding="utf-8"?>
<ole:tux-mapping xmlns:ole="http://www.ole.co.in/TUX" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xslt="http://www.w3.org/1999/XSL/Transform/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" exclude-result-prefixes="mml ole xlink" version="0.1">

<ole:wrap-tags math="tmpl[(((@templateType = '16' and @variation = '112')
                            or (@templateType = '15' and @variation = '113')

                            )
                            and descendant::*[matches(., '(∑|∫|∲|∯|∰)')] and descendant::sym)
                          ]">
  <mstyle>
    <munderover>
      <ole:element math="child::*[matches(., '(∑|∫|∲|∯|∰)')][name(preceding-sibling::*[1])='sym']"/>
      <ole:element math="line[2]"/>
      <ole:element math="line[3]"/>      
    </munderover>
     <ole:element math="line[1]"/>
  </mstyle>
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[(((@templateType = '16' and @variation = '80')
                          or (@templateType = '15' and @variation = '81')
                          or (@templateType = '15' and @variation = '82')
                          or (@templateType = '15' and @variation = '83')
                          or (@templateType = '15' and @variation = '85')
                          or (@templateType = '15' and @variation = '86')
                          or (@templateType = '15' and @variation = '87')
                          or (@templateType = '15' and @variation = '89')
                          or (@templateType = '15' and @variation = '93')
                          )
                          and descendant::*[matches(., '(∑|∫|∲|∯|∰)')] 
                          and descendant::sym)                          
                          ]">
  <mstyle>
    <munder>
      <ole:element math="child::*[matches(., '(∑|∫|∲|∯|∰)')][name(preceding-sibling::*[1])='sym']"/>
      <ole:element math="line[2]"/>
      <ole:element math="line[3]"/>      
    </munder>
     <ole:element math="line[1]"/>
  </mstyle>
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[(((@templateType = '24' and @variation = '0')
                             or (@templateType = '25' and @variation = '0')
                            )
                            and sub[following-sibling::*[2][name()='mo']]
                            )
                          ]">
  <munder>
    <munder>
      <mrow>
        <ole:element math="child::*[1]"/>
        <ole:element math="child::*[4][preceding-sibling::sub]"/>
      </mrow>
    </munder>
    <ole:element math="child::*[3]"/>        
  </munder>
</ole:wrap-tags>                          

<ole:wrap-tags math="tmpl[(((@templateType = '24' and @variation = '1')
                             or (@templateType = '25' and @variation = '1')
                            )
                            and sub[following-sibling::*[2][name()='mo']]
                            )]">
  <mover>
    <mover>
      <mrow>
        <ole:element math="child::*[1]"/>
        <ole:element math="child::*[4][preceding-sibling::sub]"/>
      </mrow>
    </mover>
    <ole:element math="child::*[3]"/>        
  </mover>
</ole:wrap-tags>                          

<ole:wrap-tags math="tmpl[(((@templateType = '16' and @variation = '64')
                             or (@templateType = '15' and @variation = '1')
                             or (@templateType = '15' and @variation = '2')
                             or (@templateType = '15' and @variation = '5')
                             or (@templateType = '15' and @variation = '6')
                             or (@templateType = '15' and @variation = '7')
                             or (@templateType = '15' and @variation = '9') 
                             or (@templateType = '15' and @variation = '13')
                             )
                             and descendant::*[matches(., '(∑|∫|∲|∯|∰)')] 
                             and descendant::sym)
                         ]">
  <mstyle>   
    <mrow>
      <ole:move tagname="{name(child::*[matches(., '(∑|∫|∲|∯|∰)')][name(preceding-sibling::*[1])='sym'])}" math="child::*[matches(., '(∑|∫|∲|∯|∰)')][name(preceding-sibling::*[1])='sym']" mode="move"/>             
      <ole:do/>
    </mrow>
  </mstyle>
</ole:wrap-tags>


<ole:wrap-tags math="tmpl[(((@templateType = '16' and @variation = '16')
                             or (@templateType = '15' and @variation = '17')
                             or (@templateType = '15' and @variation = '18')
                             or (@templateType = '15' and @variation = '19')
                             or (@templateType = '15' and @variation = '21')
                             or (@templateType = '15' and @variation = '22')
                             or (@templateType = '15' and @variation = '23')
                             or (@templateType = '15' and @variation = '25')
                             or (@templateType = '15' and @variation = '29')
                             )
                             and descendant::*[matches(., '(∑|∫|∲|∯|∰)')] 
                             and descendant::sym)
                          ]">
  <mstyle>
    <mrow>  
      <msub>
        <ole:element math="child::*[matches(., '(∑|∫|∲|∯|∰)')][name(preceding-sibling::*[1])='sym']"/>
        <ole:element math="line[2]"/>
        <ole:element math="line[3]"/>      
      </msub>
       <ole:element math="line[1]"/>
      </mrow>
  </mstyle>
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[(@templateType = '16' and @variation = '48' 
