      <msubsup>
        <ole:element math="child::*[matches(., '(∑|∫|∲|∯|∰)')][name(preceding-sibling::*[1])='sym']"/>
        <ole:element math="line[2]"/>
        <ole:element math="line[3]"/>      
      </msubsup>
       <ole:element math="line[1]"/>
    </mrow>
  </mstyle>
</ole:wrap-tags>

