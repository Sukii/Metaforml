<?xml version="1.0" encoding="utf-8"?>
<ole:tux-mapping xmlns:ole="http://www.ole.co.in/TUX" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xslt="http://www.w3.org/1999/XSL/Transform/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" exclude-result-prefixes="mml ole xlink" version="0.1">



<!--mfrac-->
<ole:wrap-tags math="tmpl[@templateType='11' and @variation='0']">  
  <mfrac>      
    <ole:element math="line[1]/*"/>
    <ole:element math="line[2]/*"/>      
  </mfrac>  
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[@templateType='11' and @variation='1']">   
    <mstyle scriptlevel="+1">
      <mfrac>        
        <ole:element math="line[1]/*"/>
        <ole:element math="line[2]/*"/>        
      </mfrac>
    </mstyle>
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[@templateType='11' and @variation='2']">   
  <mstyle scriptlevel="+1">
    <mfrac bevelled="true">
      <ole:element math="line[1]/*"/>
      <ole:element math="line[2]/*"/>        
    </mfrac>
  </mstyle>  
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[@templateType='11' and @variation='3']">
  <mfrac bevelled="true">    
    <ole:element math="line[1]/*"/>
    <ole:element math="line[2]/*"/>    
  </mfrac>
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[@templateType='11' and @variation='6']">        
  <ole:element math="line[1]/*"/>    
  <mo>/</mo>    
  <ole:element math="line[2]/*"/>      
</ole:wrap-tags>
</ole:tux-mapping>