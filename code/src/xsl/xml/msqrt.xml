<?xml version="1.0" encoding="utf-8"?>
<ole:tux-mapping xmlns:ole="http://www.ole.co.in/TUX" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xslt="http://www.w3.org/1999/XSL/Transform/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" exclude-result-prefixes="mml ole xlink" version="0.1">


<ole:wrap-tags math="tmpl[@templateType='10' and @variation='0']">        
  <msqrt>
    <ole:do/>
  </msqrt>
</ole:wrap-tags>

<ole:wrap-tags math="tmpl[@templateType='10' and @variation='1']">        
  <msqrt>
    <ole:do/>
  </msqrt>  
</ole:wrap-tags>

<ole:wrap-tags math="/">
  <math>
      <ole:do/>
  </math>
</ole:wrap-tags>

<ole:wrap-tags math="line[count(*)>0]">
  <mrow>
    <ole:do/>  
  </mrow>
</ole:wrap-tags>


<ole:wrap-tags math="line[count(*)=0  
                      and not(parent::tmpl[@templateType = '28' and @variation='1'])
                      and not(parent::tmpl[@templateType='27' and @variation='1' and child::sub])
                      and not(parent::tmpl[@templateType='27' and @variation='1' and child::sub2])
  ]">      
</ole:wrap-tags>  


<ole:wrap-tags math="sub2">
    <ole:do/>
</ole:wrap-tags>

</ole:tux-mapping>